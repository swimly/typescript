{
  "name": "mui",
  "version": "1.0.0",
  "description": "",
  "main": "dist/mui.min.js",
  "scripts": {
    "build:dev": "cross-env ENV=development rollup -wc",
    "build:prod": "cross-env ENV=producrion rollup -c rollup.config.prod.js",
    "doc": "start-storybook -p 6006",
    "build:doc": "build-storybook",
    "generate": "plop",
    "push:gitee": "git push gitee master",
    "push:github": "git push github master",
    "pull:gitee": "git pull gitee master",
    "pull:github": "git pull github master",
    "push": "concurrently \"yarn push:github\" \"yarn push:gitee\"",
    "pull": "concurrently \"yarn pull:github\" \"yarn pull:gitee\"",
    "dev": "concurrently \"yarn build:dev\" \"yarn doc\""
  },
  "theme": "red",
  "var": true,
  "browserslist": [
    "last 2 version",
    ">1%",
    "ios 7"
  ],
  "keywords": [],
  "extra": [
    "toast"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@rollup/plugin-image": "^2.0.6",
    "@storybook/addon-actions": "^6.3.0",
    "@storybook/addon-essentials": "^6.3.0",
    "@storybook/addon-links": "^6.3.0",
    "@storybook/html": "^6.3.0",
    "autoprefixer": "^10.2.6",
    "babel-loader": "^8.2.2",
    "concurrently": "^6.2.0",
    "cross-env": "^7.0.3",
    "cssnano": "^5.0.6",
    "plop": "^2.7.4",
    "postcss": "^8.3.5",
    "postcss-color-mod-function": "^3.0.3",
    "postcss-functions": "^4.0.2",
    "postcss-import": "^14.0.2",
    "postcss-mixins": "^8.1.0",
    "postcss-modules": "^4.1.3",
    "postcss-nested": "^5.0.5",
    "postcss-prepend-imports": "^1.0.1",
    "postcss-pxtorem": "^6.0.0",
    "postcss-simple-vars": "^6.0.3",
    "rollup": "^2.52.2",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-filesize": "^9.1.1",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-multi-input": "^1.3.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-progress": "^1.1.2",
    "rollup-plugin-pug": "^1.1.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "tslib": "^2.3.0",
    "typescript": "^4.3.4"
  },
  "dependencies": {
    "css-color-converter": "^2.0.0"
  }
}
